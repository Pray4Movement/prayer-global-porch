#: pages/pray/stacker-text.php:9121
#, fuzzy
msgid "Isaiah 43:11"
msgstr "اشعياء 11: 9"

#: pages/pray/stacker-text.php:9122
#, fuzzy
msgctxt "Isaiah 43:11"
msgid "I, I am the LORD, and besides me there is no savior."
