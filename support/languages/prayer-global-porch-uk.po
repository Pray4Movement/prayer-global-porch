#: pages/pray/stacker-text.php:505
#, fuzzy
msgid "Matthew 5:11-12"
msgstr "Матвія 28:18-20"

#: pages/pray/stacker-text.php:506
#, fuzzy
msgctxt "Matthew 5:11-12"
